
@{
    ViewData["Title"] = "Login";
}

<section class="clean-block clean-form dark" style="margin-top:100px;">
    <div class="container">
        <div class="block-heading">
            <h2 class="text-info">Login</h2>
            <p target="<a href=&quot;https://google.pt">Se ainda não se registou, por favor clique <a href="registration.html">aqui</a></p>
        </div>
        <form method="post">
            <div class="form-group"><label>Email</label><input class="form-control" type="text"></div>
            <div class="form-group"><label>Password</label><input class="form-control" type="text"></div>
            <div class="form-group"><button class="btn btn-primary btn-block" type="button">Login</button></div>
        </form>
    </div>
</section>
